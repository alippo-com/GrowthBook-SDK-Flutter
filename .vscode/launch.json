    "version": "0.2.0",
    "configurations": [

        {
            "name": "example",
            "request": "launch",
            "type": "dart",
            "program": "example/lib/main.dart",
            "args": [
                "--dart-define",
                "GROWTHBOOK_API_KEY=your_key",
                "--dart-define",
                "GROWTHBOOK_API_HOST=your_api_host"
            ]
        },
        {
            "name": "example web",
            "request": "launch",
            "type": "dart",
            "program": "example/lib/main.dart",
            "args": [
                "--web-browser-flag",
                "--disable-web-security",
                "--web-port=8080",
                "--dart-define",
                "GROWTHBOOK_API_KEY=your_key",
                "--dart-define",
                "GROWTHBOOK_API_HOST=your_api_host"
            ]
        },
        {
            "name": "example (profile mode)",
            "request": "launch",
            "type": "dart",
            "flutterMode": "profile"
        },
        {
            "name": "example (release mode)",
            "request": "launch",
            "type": "dart",
            "flutterMode": "release"
        },
